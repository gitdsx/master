package com.jy.entity.system.channels;

import org.apache.ibatis.type.Alias;

import java.util.Date;

/**
 * 第三方数据属性
 */
@Alias("ThirdPayInfo")
public class ThirdPayInfo {

    private String batchNo; // 交易批次号
    private String payNo; //商户支付订单号
    private String payWay;//交易类型 0：出票 1：微信 2：银联 3：得仕通
    private String payFlowNo ;//交易流水号
    private String transMoney ;//交易金额
    private String flowCode; //系统流水号
    private String tradeTime ; //交易时间
    private String openId ; //第三方账户识别号

    public String getBatchNo() {
        return batchNo;
    }

    public void setBatchNo(String batchNo) {
        this.batchNo = batchNo;
    }

    public String getPayNo() {
        return payNo;
    }

    public void setPayNo(String payNo) {
        this.payNo = payNo;
    }

    public String getPayFlowNo() {
        return payFlowNo;
    }

    public void setPayFlowNo(String payFlowNo) {
        this.payFlowNo = payFlowNo;
    }

    public String getTransMoney() {
        return transMoney;
    }

    public void setTransMoney(String transMoney) {
        this.transMoney = transMoney;
    }


    public String getFlowCode() {
        return flowCode;
    }

    public void setFlowCode(String flowCode) {
        this.flowCode = flowCode;
    }

    public String getTradeTime() {
        return tradeTime;
    }

    public void setTradeTime(String tradeTime) {
        this.tradeTime = tradeTime;
    }

    public String getOpenId() {
        return openId;
    }

    public void setOpenId(String openId) {
        this.openId = openId;
    }

    public String getPayWay() {
        return payWay;
    }

    public void setPayWay(String payWay) {
        this.payWay = payWay;
    }
}
