	</select>
	<select id="sumSalesMerchant" resultType="Double"  resultMap="sumSales">

		select IFNULL(sum(b.money),0.0) as money from DC_CP_OrderInfo b LEFT JOIN (select m.M_CPUserid as userId,j.createTime from MP_MerchantInfo m LEFT JOIN JY_BASE_ACCOUNT j ON m.M_AccountId=j.id)  u ON b.userId=u.userId
		where ((b.userId in
		<foreach item="item" index="index" collection="list" open="(" separator="," close=")">
			#{item}
		</foreach>
		AND b.schemeType =1
		)
		OR
		(b.schemeType in (select d.schemeType from  MP_MerchantExtend d where d.cpUserId in
		<foreach item="item" index="index" collection="list" open="(" separator="," close=")">
			#{item}
		</foreach>
		)))
		AND b.buyTime>u.createTime
		AND b.status in(17,19,20,21,22)
		AND b.buyTime
		BETWEEN DATE_FORMAT(#{beginTime},'%Y-%m-%d %H:%i:%s')
		and DATE_FORMAT(#{endTime},'%Y-%m-%d %H:%i:%s')
	</select>
