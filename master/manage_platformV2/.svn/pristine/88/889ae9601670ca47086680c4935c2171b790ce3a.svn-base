			and	t.SCR_MERCHANTID=m.M_ID
			AND  t.SCR_DATE >=DATE_FORMAT(m.createTime,'%Y-%m-%d')
			and m.M_AccountId in
			<foreach item="item" index="index" collection="accountList" open="(" separator="," close=")">
